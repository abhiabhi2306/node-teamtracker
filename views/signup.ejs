<form action="/signup" method="POST" class="bs-component">

<input type="text" class="form-control" placeholder="Desired Username" name="username" required>
<input type="password" class="form-control" placeholder="Desired Password" id="password" name="password" required>
<input type="text" class="form-control" placeholder="email" name="email" required>
<br>
<button type="submit" class="btn btn-primary">Sign Up</button>
</form>
